{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT e.*\n            FROM event e\n              JOIN program p ON e.program_id = p.id\n              LEFT JOIN ven_program vp ON p.id = vp.program_id\n            WHERE e.id = $1\n              AND (NOT $2 OR vp.ven_id IS NULL OR vp.ven_id = ANY($3)) -- Filter for VEN ids\n            ",
